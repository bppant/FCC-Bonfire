function palindrome(str) {
    
    var strMod= str.toLowerCase().replace('_','').split(/\W*/).join("");
    var len = strMod.length;
   
    for ( var i = 0; i < Math.floor(len/2); i++ ) {
        if (strMod[i] !== strMod[len - 1 - i]) {
            return false;
        }
    }
    return true;
}
  
palindrome("Race Car");
